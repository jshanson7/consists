{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "arrow-parens": 0,
    "comma-dangle": 0,
    "comma-spacing": 0,
    "func-names": 0,
    "no-cond-assign": 0,
    "no-plusplus": 0,
    "no-self-compare": 0,
    "no-undef": 0,
    "no-use-before-define": 0,
    "object-curly-spacing": 0,
    "prefer-arrow-callback": 0,
    "prefer-const": 0
  }
}
